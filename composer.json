{
    "name": "nanofelis/json-rpc-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a JSON RPC 2.0 implementation for Symfony",
    "keywords": ["json-rpc"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Soullier",
            "email": "gabriel.soullier@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "symfony/event-dispatcher": "^5.4|^6.0",
        "symfony/options-resolver": "^5.4|^6.0",
        "symfony/property-access": "^5.4|^6.0",
        "symfony/serializer": "^5.4|^6.0",
        "symfony/twig-bundle": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.0",
        "symfony/var-dumper": "^5.4|^6.0",
        "phpstan/phpstan-symfony": "^1.0",
        "symfony/browser-kit": "^5.4|^6.0",
        "friendsofphp/php-cs-fixer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Nanofelis\\Bundle\\JsonRpcBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
