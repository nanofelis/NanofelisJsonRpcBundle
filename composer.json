{
    "name": "nanofelis/json-rpc-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a json rpc 2.0 implementation for Symfony",
    "keywords": ["json-rpc"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Soullier",
            "email": "gabriel.soullier@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "symfony/event-dispatcher": "^4.0|^5.0",
        "symfony/options-resolver": "^4.0|^5.0",
        "symfony/property-access": "^4.0|^5.0",
        "symfony/serializer": "^4.0|^5.0",
        "symfony/validator": "^4.0|^5.0"
    },
    "require-dev": {
        "ext-intl": "*",
        "symfony/phpunit-bridge": "^4.0|^5.0",
        "symfony/var-dumper": "^4.0|^5.0",
        "phpstan/phpstan-symfony": "^0.11.6",
        "symfony/browser-kit": "^4.0|^5.0",
        "sensio/framework-extra-bundle": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Nanofelis\\Bundle\\JsonRpcBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "Tests/" }
    }
}
